{
  "name": "wuzapi-manager",
  "private": true,
  "version": "0.0.7",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint .",
    "lint:security": "eslint .",
    "audit:security": "npm audit --audit-level=moderate",
    "security:scan": "./scripts/security-scan.sh",
    "security:quick": "./scripts/security-audit-quick.sh",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run src/**/*.integration.test.{ts,tsx}",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "server:build": "cd server && npm install --production",
    "migrate:subscriptions": "node server/scripts/migrate-users-to-default-plan.js",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "setup": "npm install && npm run server:install",
    "deploy:build": "npm run build:production && npm run server:build",
    "deploy:check": "npm run lint && npm run build:production",
    "clean": "rm -rf dist && rm -rf server/node_modules",
    "clean:install": "npm run clean && npm install && npm run server:install",
    "generate": "node scripts/generate.cjs",
    "generate:help": "node scripts/generate.cjs --help",
    "generate:test": "node scripts/test-generate.cjs",
    "docker:build": "./deploy-multiarch.sh",
    "docker:deploy": "./deploy.sh",
    "docker:check": "./scripts/check-deployment.sh",
    "docker:status": "docker service ps wuzapi-manager_wuzapi-manager",
    "docker:logs": "docker service logs wuzapi-manager_wuzapi-manager -f",
    "docker:remove": "docker stack rm wuzapi-manager",
    "deploy:official": "./deploy-multiarch.sh",
    "deploy:production": "./deploy.sh",
    "release": "./scripts/release.sh",
    "release:dry-run": "./scripts/release.sh --dry-run",
    "validate:commits": "./scripts/validate-commit.sh",
    "validate:commits:last": "./scripts/validate-commit.sh --last",
    "validate:commits:all": "./scripts/validate-commit.sh --all",
    "changelog:generate": "./scripts/generate-changelog.sh",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "landing:update": "node scripts/update-landing-page.cjs"
  },
