    "concurrently": "^8.2.2",
    "cypress": "^15.6.0",
    "eslint": "^9.9.0",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^15.9.0",
    "jsdom": "^27.1.0",
    "lovable-tagger": "^1.1.7",
